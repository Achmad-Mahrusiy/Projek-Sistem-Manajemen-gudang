<!-- <!DOCTYPE html> --> <!--declares HTML5 tipe dokumen-->
<!-- <html>  --> <!--element penting untuk setiap halaman-->
<!-- head --> <!--isi metadata (tidak terlihat di halaman)-->
<!-- <body> --> <!--isi informasi yang terlihat disetiap halaman.-->



<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <h1>Dokumen</h1>
  <h2>Halaman kedua<hr>Berikut tentang paragraf html<h2> <!--<hr> tanda ini memberi horizontal line-->
  <p>Paragraf awal</p>  <!--<P> tanda ini adalah paragraf, bisa diberi angka setiap paragrafnya tanpa mengubah bentuk paragrafnya-->
  <p>Paragraf kedua<br> digunakan untuk muncul dibaris berikutnya</p>  <!--<br> tanda ini adalah line break -->
  <pre>mengikuti setiap barisan    
      barisan pertama
      barisan kedua
      barisan ketiga
      barisan keempat</pre> <!--<pre> tanda ini mengikuti pola baris yang sesuai dengan yang kita ketik-->
  <!-- ini adalah comment untuk html -->
</body>

</html>
